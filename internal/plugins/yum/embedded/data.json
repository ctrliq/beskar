{
    "routes": [
        {
            "pattern": "/(artifacts/yum/[0-9A-Za-z-_./]{1,})/repo/repodata/([^/]+)$",
            "blobtype": "repodata",
            "methods": [
                "GET",
                "HEAD"
            ]
        },
        {
            "pattern": "/(artifacts/yum/[0-9A-Za-z-_./]{1,})/repo/.*?([^/]+\\.[s]?rpm)$",
            "blobtype": "packages",
            "methods": [
                "GET",
                "HEAD"
            ]
        },
        {
            "pattern": "/artifacts/yum/api/v1/doc/(.*)",
            "body": false
        },
        {
            "pattern": "/artifacts/yum/api/v1/(.*)",
            "body": true,
            "body_key": "repository"
        }
    ],
    "mediatype": {
        "rpm": "application/vnd.ciq.rpm.package.v1.rpm",
        "repomd": "application/vnd.ciq.rpm.repomd.v1.xml",
        "repomdasc": "application/vnd.ciq.rpm.repomd.v1.xml.asc"
    }
}